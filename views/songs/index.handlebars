{{#each songs}}
<div class="card card-body">
  <h4>{{title}}</h4>
  <p class="song-info">
    <strong> Artist: </strong> {{artist}}</p>
  <p class="song-info">
    <strong>Key: </strong> {{songKey}}</p>
  <div class="row container">
    <a class="btn btn-dark mb-2 edit-btn" href="/songs/edit/{{id}}">Edit</a>
    <form class="edit-btn delete" method="post" action="/songs/{{id}}?_method=DELETE">
      <input type="hidden" name="_method" value="DELETE">
      <input type="submit" class="btn btn-danger btn-block" value="Delete">
    </form>
  </div>
</div>
{{else}}
<p>No songs listed</p>
{{/each}}